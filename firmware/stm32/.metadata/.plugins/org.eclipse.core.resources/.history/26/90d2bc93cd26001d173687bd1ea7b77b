#include <fifo.h>

void FIFO_init(FIFO* fifo, uint8_t* array, uint32_t size_in_bytes){
	fifo->buffer = array;
	fifo->start = 0;
	fifo->end = 0;
	fifo->size = size_in_bytes << 3;
}

void FIFO_push(FIFO* fifo, uint8_t bit){

}
